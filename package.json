{
  "name": "frame-code-platform",
  "version": "1.0.0",
  "description": "Sistema de workspace isolado por task com Docker",
  "main": "dist/index.js",
  "bin": {
    "frame-code": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "api:dev": "ts-node src/api/main.ts",
    "api:start": "node dist/api/main.js",
    "web:dev": "vite",
    "web:build": "vite build",
    "web:preview": "vite preview",
    "dev:all": "concurrently \"npm run api:dev\" \"npm run web:dev\""
  },
  "keywords": [
    "agents",
    "workspace",
    "docker",
    "isolation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/websockets": "^11.1.11",
    "@preact/signals": "^2.5.1",
    "@tailwindcss/postcss": "^4.1.18",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "dockerode": "^4.0.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.1",
    "preact": "^10.28.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tar-fs": "^3.0.0",
    "uuid": "^9.0.0",
    "wouter": "^3.9.0"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/tar-fs": "^2.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
