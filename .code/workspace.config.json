{
    "defaultImage":  "node:20-alpine",
    "workspaceBaseDir":  ".workspace",
    "autoCleanup":  true,
    "sessionTimeout":  86400000,
    "maxConcurrentWorkspaces":  5,
    "docker":  {
                   "networkMode":  "bridge",
                   "portBindings":  {

                                    },
                   "volumeBindings":  {
                                          ".":  "/workspace"
                                      },
                   "environment":  {
                                       "NODE_ENV":  "development"
                                   }
               },
    "persist":  {
                    "enabled":  true,
                    "path":  ".workspace",
                    "compression":  true
                }
}